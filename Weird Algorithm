#include<bits/stdc++.h>
using namespace std;
 
#define endl "\n"
#define ll long long
 
int main(){
  ios_base::sync_with_stdio(false);
  cin.tie(NULL); cout.tie(NULL);
  
  int tt = 1;
  // cin >> tt;
 
  while(tt--){
    ll  n; cin >> n;
    while(n != 1){
      cout << n  << " ";
      if(n % 2 != 0){
        n = (n * 3) +1 ;
      }
      else{
        n = n / 2;
      }
    }
    cout << 1;
}
 
return 0;
 
}
